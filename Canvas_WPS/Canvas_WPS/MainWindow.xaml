<Window x:Class="Canvas_WPS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Canvas_WPS"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button Click="Btn1_Click" Panel.ZIndex="0" x:Name="Btn1" Canvas.Bottom="150" Canvas.Left="100" Content="1.Click Here!!" Height="200" Width="100"></Button>
        <Button Click="Btn2_Click" Panel.ZIndex="0" x:Name="Btn2" Canvas.Bottom="50" Canvas.Left="150" Content="2.Click Here!!" Height="200" Width="100"></Button>
        <Rectangle Fill="Aqua" Height="50" Width="30"></Rectangle>
        <Ellipse Fill="Beige" Height="200" Width="599"></Ellipse>
    </Canvas>
</Window>
